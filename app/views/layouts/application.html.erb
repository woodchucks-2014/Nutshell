<!DOCTYPE html>
<html>
<head>
  <title>Nutshell</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>

    <% if session[:user_id] %>
      <%= link_to 'logout', sign_out_path %>
    <% end %>
    </nav>
<%= yield %>

</body>
</html>
